  "bugs": {
    "url": "https://github.com/Nevon/demo-slack-npm-notifier/issues"
  },
  "homepage": "https://github.com/Nevon/demo-slack-npm-notifier#readme",
  "dependencies": {
    "@babel/runtime": "^7.13.17",
    "@slack/webhook": "^5.0.3",
    "axios": "^0.21.1",
    "browserify": "^17.0.0",
    "cookie-parser": "^1.4.5",
    "css-loader": "^5.2.4",
    "express": "^4.17.1",
    "kafkajs": "^1.14.0",
    "nodemon": "^2.0.7",
    "npm-hook-receiver": "^1.0.0",
    "prop-types": "^15.7.2",
